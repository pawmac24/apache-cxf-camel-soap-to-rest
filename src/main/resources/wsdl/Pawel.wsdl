			<wsdl:input>
				<soap:body use="literal" />
			</wsdl:input>
			<wsdl:output>
				<soap:body use="literal" />
			</wsdl:output>
		</wsdl:operation>
	</wsdl:binding>
	<wsdl:service name="Pawel">
		<wsdl:port binding="tns:PawelSOAP" name="PawelSOAP">
			<soap:address location="http://localhost:8080/apache-cxf-jax-ws-demo/services/pawel/" />
		</wsdl:port>
	</wsdl:service>
