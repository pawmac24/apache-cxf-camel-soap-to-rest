<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<wsdl:definitions xmlns:pawel="http://www.pm.company.com/schema/Pawel"
	xmlns:soap="http://schemas.xmlsoap.org/wsdl/soap/" xmlns:tns="http://www.pm.company.com/Pawel/"
	xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/" xmlns:xsd="http://www.w3.org/2001/XMLSchema"
	name="Pawel" targetNamespace="http://www.pm.company.com/Pawel/">
	<wsdl:types>
		<xsd:schema targetNamespace="http://www.pm.company.com/Pawel/">
			<xsd:import namespace="http://www.pm.company.com/schema/Pawel" schemaLocation="../xsd/Pawel.xsd"></xsd:import>
		</xsd:schema>
	</wsdl:types>
	<wsdl:message name="FirstOperationRequest">
		<wsdl:part element="pawel:FirstOperation" name="parameters" />
	</wsdl:message>
	<wsdl:message name="FirstOperationResponse">
		<wsdl:part element="pawel:FirstOperationResponse" name="parameters" />
	</wsdl:message>
	<wsdl:portType name="Pawel">
		<wsdl:operation name="FirstOperation">
			<wsdl:input message="tns:FirstOperationRequest" />
			<wsdl:output message="tns:FirstOperationResponse" />
		</wsdl:operation>
	</wsdl:portType>
